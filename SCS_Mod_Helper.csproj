    <None Remove="Images\ListBox.png" />
    <None Remove="Images\Paste.png" />
    <None Remove="Images\Settings.png" />
    <None Remove="Images\Up.png" />
    <None Remove="Images\Web.png" />
    <None Remove="Images\WebReferenceFolder.png" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="App.config" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="10.0.0-rc.1.25451.107" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\Add.png" />
    <Resource Include="Images\AddFile.png" />
    <Resource Include="Images\AddRow.png" />
    <Resource Include="Images\Copy.png" />
    <Resource Include="Images\Delete.png" />
    <Resource Include="Images\DeleteFile.png" />
