<Application
	x:Class="SCS_Mod_Helper.App"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:s="clr-namespace:System;assembly=mscorlib"
	xmlns:local="clr-namespace:SCS_Mod_Helper"
	xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
	StartupUri="Main/NaviMainWindow.xaml"
	DispatcherUnhandledException="UnhandledException"
	Exit="OnExit">
	<Application.Resources>
		<ResourceDictionary>
			<s:String
				x:Key="Version">V1.0</s:String>
			
			<ResourceDictionary.MergedDictionaries>
				<ui:ThemesDictionary
					Theme="Light" />
				<ui:ControlsDictionary />
				<ResourceDictionary
					Source="/Language/en-US_English(US).xaml" />
				<ResourceDictionary
					Source="/Language/zh-CN_简体中文.xaml" />
				<ResourceDictionary
					Source="/Theme/Dark.xaml" />
				<ResourceDictionary
					Source="/Theme/Light.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
    </Application.Resources>
</Application>
